{
  "rules": {
      "indent": [
          1,
          2
      ],
      "quotes": [
          1,
          "single"
      ],
      "linebreak-style": [
          2,
          "unix"
      ],
      "semi": [
          1,
          "never"
      ],
      "no-unused-vars": 1,
      "no-console": 1
  },
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
      "es6": true,
      "node": true,
      "browser": true,
      "jasmine": true
  },
  "extends": "eslint:recommended"
}
